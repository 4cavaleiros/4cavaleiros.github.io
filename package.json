{
  "name": "4cavaleiros",
  "private": true,
  "description": "4Cavaleiros Game",
  "version": "0.1.0",
  "author": "Cesar Augusto <cesardeazevedo@outlook.com>",
  "license": "BSD",
  "keywords": [
    "4cavaleiros",
    "game"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/4cavaleiros"
  },
  "scripts": {
    "build": "gatsby build",
    "build:intl": "rimraf .i18n && NODE_ENV=intl $(npm bin)/babel --verbose --extensions=.tsx,.ts src && node src/utils/translations.js",
    "develop": "gatsby develop",
    "deploy": "yarn build && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-styled-components": "^3.5.0",
    "gatsby-plugin-google-gtag": "^2.4.0",
    "gatsby-theme-material-ui": "^1.0.13",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "gh-pages": "2.2.0",
    "hls.js": "^0.14.17",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.10.6",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.6",
    "@types/styled-components": "^5.1.4",
    "@types/react-intl": "^3.0.0",
    "@babel/cli": "^7.12.8",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "babel-preset-gatsby": "^0.8.0",
    "babel-plugin-react-intl": "^7.5.9",
    "prettier": "2.1.2",
    "rimraf": "^3.0.2",
    "react-intl-translations-manager": "^5.0.3"
  },
  "babel": {
    "presets": [
      "babel-preset-gatsby"
    ],
    "env": {
      "intl": {
        "plugins": [
          [
            "@babel/plugin-transform-typescript",
            {
              "isTSX": true
            }
          ],
          [
            "react-intl",
            {
              "messagesDir": "./.i18n"
            }
          ]
        ]
      }
    }
  }
}
